<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.qaitdevlabs.ptpat.model.SubCategory" table="sub_category">
		<id name="subCategoryId" type="java.lang.Long">
			<column name="sub_category_id" />
			<generator class="native" />
		</id>

		<property name="subCategoryName" type="string" unique="true">
			<column name="sub_category_name" length="40" not-null="true"
				unique="true" />
		</property>
	</class>
</hibernate-mapping>