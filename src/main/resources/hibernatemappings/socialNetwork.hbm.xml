<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.qaitdevlabs.qualityassessor.model.SocialNetwork" table="social_network">
        <id name="socialNetworkId" type="java.lang.Long">
            <column name="social_network_id" />
            <generator class="native" />
        </id>
        <property name="socialSiteName" type="string" unique="true">
            <column name="social_site_name" length="25" not-null="true" />
        </property>
        <property name="socialSiteId" type="string" unique="true">
            <column name="social_site_id" length="64" not-null="true" />
        </property>
        <many-to-one name="user" class="com.qaitdevlabs.qualityassessor.model.User">
            <column name="user_id" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>